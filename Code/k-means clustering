from sklearn.cluster import KMeans

kmeans = KMeans(n_clusters=3, random_state=0).fit(angle)
labels = kmeans.labels_
center = kmeans.cluster_centers_
centerx = center[:,0]
centery = center[:,1]

plt.scatter(angle['xmax'], angle['zdelta'], c = labels)
plt.scatter(centerx, centery, marker = 'D', color = 'red')
plt.xlabel('max x value in degree')
plt.ylabel('delta z value in degree')
plt.xlim(90, 370)
plt.ylim(0, 60)
plt.plot()
plt.show()
